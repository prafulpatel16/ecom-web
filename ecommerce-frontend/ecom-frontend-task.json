{
    "family": "ecom-web-frontend",
    "containerDefinitions": [
        {
            "name": "frontend",
            "image": "739313151559.dkr.ecr.us-west-1.amazonaws.com/ecom-web-frontend:latest",
            "cpu": 0,
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 80,
                    "protocol": "tcp"
                }
            ],
            "essential": true,
            "environment": [
                {
                    "name": "REACT_APP_BACKEND_SERVICE",
                    "value": "http://ecom-service.ecom.local:5000"
                }
            ],
            "mountPoints": [],
            "volumesFrom": [],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "ecs-frontend-loggroup",
                    "awslogs-create-group": "true",
                    "awslogs-region": "us-west-1",
                    "awslogs-stream-prefix": "frontend"
                }
            },
            "systemControls": []
        }
    ],
    "executionRoleArn": "arn:aws:iam::739313151559:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "256",
    "memory": "512"
}